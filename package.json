{
  "name": "cypress-parallel",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --fix --ext .js,.vue,.ts src"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.2",
    "axios": "^0.21.1",
    "vue": "^3.0.5",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.8"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^2.1.2",
    "@types/node": "^15.0.2",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.2.15",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.1.3",
    "vite": "^2.2.3",
    "vue-tsc": "^0.0.24"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix --ext .js,.vue,.ts src",
      "npm run build",
      "git add"
    ]
  }
}
